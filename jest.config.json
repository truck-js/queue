{
  "cacheDirectory": "<rootDir>/jest_modules",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/lib"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleFileExtensions": [
    "js"
  ],
  "roots": [
    "<rootDir>/src"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/lib"
  ]
}
